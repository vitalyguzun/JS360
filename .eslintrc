{
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      },
      "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": [
            "error", {
                "vars": "all", "args": "after-used", "ignoreRestSiblings": false
            }
        ]
    }
}
